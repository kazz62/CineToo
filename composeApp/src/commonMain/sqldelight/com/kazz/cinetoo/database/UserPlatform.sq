CREATE TABLE UserPlatform (
    id INTEGER NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    logoPath TEXT
);

selectAll:
SELECT * FROM UserPlatform;

insert:
INSERT OR REPLACE INTO UserPlatform(id, name, logoPath)
VALUES (?, ?, ?);

delete:
DELETE FROM UserPlatform WHERE id = ?;

deleteAll:
DELETE FROM UserPlatform;
